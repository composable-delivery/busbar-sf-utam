{
    "description": {
        "author": "Salesforce",
        "text": [
            "Selector: runtime_sales_cadencebuilder-actions",
            "Represents the actions on cadence builder toolbar component."
        ]
    },
    "root": true,
    "selector": {
        "css": "runtime_sales_cadencebuilder-actions"
    },
    "beforeLoad": [
        {
            "apply": "waitFor",
            "args": [
                {
                    "type": "function",
                    "predicate": [
                        {
                            "element": "root",
                            "apply": "containsElement",
                            "args": [
                                {
                                    "type": "locator",
                                    "value": {
                                        "css": "lightning-button"
                                    }
                                },
                                {
                                    "value": true
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ],
    "elements": [
        {
            "public": true,
            "name": "buttons",
            "type": "utam-lightning/pageObjects/button",
            "selector": {
                "css": "lightning-button",
                "returnAll": true
            }
        },
        {
            "public": true,
            "name": "buttonByIndex",
            "type": "utam-lightning/pageObjects/button",
            "selector": {
                "css": "lightning-button:nth-of-type(%d)",
                "args": [
                    {
                        "name": "index",
                        "type": "number"
                    }
                ]
            }
        }
    ]
}
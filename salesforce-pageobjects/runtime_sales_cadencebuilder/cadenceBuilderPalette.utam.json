{
    "description": {
        "author": "Salesforce",
        "text": [
            "Represents the Cadence Builder's Palette component."
        ]
    },
    "shadow": {
        "elements": [
            {
                "public": true,
                "name": "cadenceOverviewIcon",
                "type": "utam-lightning/pageObjects/icon",
                "selector": {
                    "css": "lightning-icon"
                }
            },
            {
                "public": true,
                "name": "cadenceOverviewText",
                "type": "utam-lightning/pageObjects/formattedText",
                "selector": {
                    "css": "lightning-formatted-text"
                }
            },
            {
                "public": true,
                "name": "accordion",
                "type": "utam-lightning/pageObjects/accordion",
                "selector": {
                    "css": "lightning-accordion"
                }
            },
            {
                "public": true,
                "name": "blockBuilderPaletteContainer",
                "type": "utam-es_block_builder/pageObjects/blockBuilderPaletteContainer",
                "selector": {
                    "css": "es_block_builder-block-builder-palette-container"
                }
            }
        ]
    },
    "methods": [
        {
            "name": "waitForPalette",
            "compose": [
                {
                    "apply": "waitFor",
                    "args": [
                        {
                            "type": "function",
                            "predicate": [
                                {
                                    "element": "blockBuilderPaletteContainer"
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}
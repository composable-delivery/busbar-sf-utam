{
    "description": {
        "author": "Salesforce",
        "text": [
            "Selector: es_block_builder-actions",
            "Represents the actions on content editor toolbar component."
        ]
    },
    "root": true,
    "selector": {
        "css": "es_block_builder-actions"
    },
    "beforeLoad": [
        {
            "apply": "waitFor",
            "args": [
                {
                    "type": "function",
                    "predicate": [
                        {
                            "element": "root",
                            "apply": "containsElement",
                            "args": [
                                {
                                    "type": "locator",
                                    "value": {
                                        "css": "lightning-button"
                                    }
                                },
                                {
                                    "value": true
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ],
    "shadow": {
        "elements": [
            {
                "name": "buttonGroup",
                "public": true,
                "type": "utam-lightning/pageObjects/buttonGroup",
                "selector": {
                    "css": "lightning-button-group"
                }
            }
        ]
    }
}
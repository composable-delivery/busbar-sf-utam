{
    "description": {
        "author": "Salesforce",
        "text": [
            "Selector: lst-breadcrumbs.",
            "Represents the Breadcrumbs on DRL.",
            "Access links"
        ]
    },
    "shadow": {
        "elements": [
            {
                "type": [
                    "clickable"
                ],
                "name": "links",
                "public": true,
                "selector": {
                    "css": "a",
                    "returnAll": true
                },
                "nullable": true
            },
            {
                "name": "nonClickableBreadcrumbs",
                "public": true,
                "selector": {
                    "css": ".slds-breadcrumb__item h1",
                    "returnAll": true
                },
                "nullable": true
            }
        ]
    }
}
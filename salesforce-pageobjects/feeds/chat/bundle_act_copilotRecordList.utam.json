                "wait": true
            },
            {
                "name": "lstCheckboxComponent",
                "selector": {
                    "css": "lstCheckbox"
                },
                "description": "Represents a checkbox field component that is disabled and has a hidden label.",
                "nullable": true,
                "wait": true
            },
            {

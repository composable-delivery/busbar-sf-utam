            {
                "name": "forceLookupComponent",
                "selector": {
                    "css": "force-lookup"
                },
                "description": "Represents a lookup field component with navigable and previewable attributes.",
                "nullable": true,
                "wait": true,
                "public": true
            },
            {
                "name": "lstFormattedTextComponent",
                "selector": {
                    "css": "lstFormattedText"
                },

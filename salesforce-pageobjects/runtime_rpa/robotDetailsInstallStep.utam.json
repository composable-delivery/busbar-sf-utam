            {
                "public": true,
                "name": "robotInstallationInfo",
                "selector": {
                    "css": "div[class~='rpa-robot-details-install-step-credential-content-entry-item']:nth-of-type(%d)",
                    "args": [
                        {
                            "name": "index",
                            "type": "number"
                        }
                    ]
                }
            }
        ]
    }
}
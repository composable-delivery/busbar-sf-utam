                                },
                                "elements": [
                                    {
                                        "public": true,
                                        "name": "paletteItem",
                                        "type": "utam-webruntimedesign/pageObjects/paletteItem",
                                        "selector": {
                                            "css": "webruntimedesign-palette-item"
                                        }
                                    }
                                ]
                            }

                    "css": "webruntimedesign-search-input"
                }
            },
            {
                "name": "section",
                "type": [],
                "selector": {
                    "css": "section"
                },
                "elements": [
                    {
                        "public": true,
                        "name": "pContent",
                        "type": [],
                        "selector": {
                            "css": "p"
                        }
                    },
                    {
                        "public": true,
                        "name": "accordion",

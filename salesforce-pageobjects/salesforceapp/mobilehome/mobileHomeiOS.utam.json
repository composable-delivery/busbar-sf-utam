{
    "description": {
        "author": "Salesforce",
        "text": [
            "Selector: **/XCUIElementTypeWindow[$type = 'XCUIElementTypeNavigationBar' AND name == 'Home'$].",
            "Represents the iOS mobile home view controller.",
            "Edit the mobile home card list."
        ]
    },
    "implements": "utam-salesforceapp/pageObjects/mobilehome/mobileHome",
    "root": true,
    "platform": "native",
    "profile": [
        {
            "platform": [
                "ios_phone",
                "ios_tablet"
            ]
        }
    ],
    "selector": {
        "classchain": "**/XCUIElementTypeWindow[$type = 'XCUIElementTypeNavigationBar' AND name == 'Home'$]"
    },
    "elements": [
        {
            "name": "editButton",
            "type": [
                "clickable"
            ],
            "selector": {
                "classchain": "**/XCUIElementTypeButton[`name == 'editButton'`]"
            }
        },
        {
            "name": "homeCardList",
            "type": "utam-salesforceapp/pageObjects/mobilehome/homeCardList",
            "selector": {
                "classchain": "**/XCUIElementTypeTable[`name == 'mobileHomeTableView'`]"
            },
            "public": true
        },
        {
            "name": "continueButton",
            "nullable": true,
            "type": [
                "clickable"
            ],
            "selector": {
                "classchain": "**/XCUIElementTypeButton[`name == 'continueButton'`]"
            }
        }
    ],
    "methods": [
        {
            "name": "editHome",
            "compose": [
                {
                    "element": "editButton",
                    "apply": "click"
                }
            ]
        },
        {
            "name": "clickContinue",
            "compose": [
                {
                    "element": "continueButton",
                    "apply": "click"
                }
            ]
        },
        {
            "name": "hasContinueButton",
            "compose": [
                {
                    "element": "root",
                    "apply": "containsElement",
                    "args": [
                        {
                            "type": "locator",
                            "value": {
                                "classchain": "**/XCUIElementTypeButton[`name == 'continueButton'`]"
                            }
                        }
                    ]
                }
            ]
        },
        {
            "name": "waitForCardListToLoad",
            "compose": [
                {
                    "apply": "waitFor",
                    "args": [
                        {
                            "type": "function",
                            "predicate": [
                                {
                                    "element": "root",
                                    "apply": "isPresent"
                                },
                                {
                                    "element": "root",
                                    "apply": "containsElement",
                                    "args": [
                                        {
                                            "type": "locator",
                                            "value": {
                                                "classchain": "**/XCUIElementTypeTable[`name == 'mobileHomeTableView'`]"
                                            }
                                        }
                                    ]
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}
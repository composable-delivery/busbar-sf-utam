{
    "description": {
        "author": "Salesforce",
        "text": [
            "Selector: es_brand-brand-editor-ui",
            "Provides access to propertyPanel component"
        ]
    },
    "beforeLoad": [
        {
            "apply": "waitFor",
            "args": [
                {
                    "type": "function",
                    "predicate": [
                        {
                            "element": "root",
                            "apply": "containsElement",
                            "args": [
                                {
                                    "type": "locator",
                                    "value": {
                                        "css": "es_block_builder-block-builder-property-panel"
                                    }
                                },
                                {
                                    "value": true
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ],
    "shadow": {
        "elements": [
            {
                "public": true,
                "name": "propertyPanel",
                "type": "utam-es_block_builder/pageObjects/blockBuilderPropertyPanel",
                "selector": {
                    "css": "es_block_builder-block-builder-property-panel"
                }
            }
        ]
    }
}
{
    "description": {
        "text": [
            "Selector: .setup_platform_notificationsCustomNotifications. Allows for accessing the new notification wizard."
        ],
        "author": "Salesforce"
    },
    "root": true,
    "beforeLoad": [
        {
            "apply": "waitFor",
            "args": [
                {
                    "type": "function",
                    "predicate": [
                        {
                            "element": "root",
                            "apply": "isPresent"
                        }
                    ]
                }
            ]
        }
    ],
    "selector": {
        "css": ".setup_platform_notificationsCustomNotifications"
    },
    "methods": [
        {
            "name": "waitForNewViaWizard",
            "compose": [
                {
                    "apply": "waitFor",
                    "args": [
                        {
                            "type": "function",
                            "predicate": [
                                {
                                    "element": "root",
                                    "apply": "containsElement",
                                    "args": [
                                        {
                                            "type": "locator",
                                            "value": {
                                                "css": ".new-custom-notification-type-wizard"
                                            }
                                        }
                                    ]
                                }
                            ]
                        }
                    ]
                },
                {
                    "apply": "returnSelf"
                }
            ]
        },
        {
            "name": "clickNewViaWizard",
            "compose": [
                {
                    "element": "newViaWizard",
                    "apply": "click"
                }
            ]
        }
    ],
    "elements": [
        {
            "name": "newViaWizard",
            "public": true,
            "type": [
                "clickable"
            ],
            "selector": {
                "css": ".new-custom-notification-type-wizard"
            }
        }
    ]
}
{
    "description": {
        "author": "Salesforce",
        "text": [
            "Selector: es_property_editors-property-editor-flyout",
            "Provides access to lightning popup, schemaDrivenPropertySheetUi components."
        ]
    },
    "beforeLoad": [
        {
            "apply": "waitFor",
            "args": [
                {
                    "type": "function",
                    "predicate": [
                        {
                            "element": "root",
                            "apply": "containsElement",
                            "args": [
                                {
                                    "type": "locator",
                                    "value": {
                                        "css": "lightning-popup"
                                    }
                                },
                                {
                                    "value": false
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ],
    "elements": [
        {
            "public": true,
            "name": "popup",
            "type": "utam-lightning/pageObjects/popup",
            "selector": {
                "css": "lightning-popup"
            }
        },
        {
            "public": true,
            "name": "schemaDrivenPropertySheetUi",
            "type": "utam-es_block_builder/pageObjects/schemaDrivenPropertySheetUi",
            "selector": {
                "css": "es_block_builder-schema-driven-property-sheet-ui"
            }
        }
    ]
}
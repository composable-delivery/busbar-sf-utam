{
    "description": {
        "author": "Salesforce",
        "text": [
            "Selector: es_property_editors-color-editor",
            "Provides access to lightning input,dataBindingButton components"
        ]
    },
    "beforeLoad": [
        {
            "apply": "waitFor",
            "args": [
                {
                    "type": "function",
                    "predicate": [
                        {
                            "element": "root",
                            "apply": "containsElement",
                            "args": [
                                {
                                    "type": "locator",
                                    "value": {
                                        "css": "lightning-input"
                                    }
                                },
                                {
                                    "value": true
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ],
    "shadow": {
        "elements": [
            {
                "public": true,
                "name": "input",
                "type": "utam-lightning/pageObjects/input",
                "selector": {
                    "css": "lightning-input"
                }
            },
            {
                "public": true,
                "name": "dataBindingButton",
                "type": "utam-es_property_editors/pageObjects/dataBindingButton",
                "selector": {
                    "css": "es_property_editors-data-binding-button"
                }
            }
        ]
    }
}